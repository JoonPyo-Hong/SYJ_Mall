<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="info">
	
	<select id="InfoSelect"
		resultType="com.test.SYJ_Mall.info.InfoDTO">
		select * from dbo.kakao_info with(nolock)
	</select>
	<select id="InfoSelect_en"
		resultType="com.test.SYJ_Mall.info.InfoDTO">
		select * from dbo.kakao_info_en with(nolock)
	</select> 
	<select id="InfoSelect_jp"
		resultType="com.test.SYJ_Mall.info.InfoDTO">
		select * from dbo.kakao_info_jp with(nolock)
	</select> 
	<select id="InfoSelect_cn"
		resultType="com.test.SYJ_Mall.info.InfoDTO">
		select * from dbo.kakao_info_cn with(nolock)
	</select> 
	<select id="InfoSelect_cn2"
		resultType="com.test.SYJ_Mall.info.InfoDTO">
		select * from dbo.kakao_info_cn2 with(nolock)
	</select>  
</mapper>