<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="board">

	<select id="noticelist" resultType="com.test.board.BoardDTO">
		SELECT
		seq, qubn, title, content, reg_id, reg_de
		FROM NOTICE
	</select>


</mapper>